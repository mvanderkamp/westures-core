{
  "markdown": {
    "idInHeadings": true
  },
  "opts": {
    "destination": "docs",
    "mainpagetitle": "Home | Westures Core Documentation",
    "package": "package.json",
    "readme": "README.md",
    "recurse": true,
    "template": "node_modules/tui-jsdoc-template"
  },
  "plugins": [],
  "recurseDepth": 10,
  "source": {
    "include": [
      "src",
      "index.js"
    ],
    "includePattern": ".js$"
  },
  "sourceType": "module",
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc"]
  },
  "templates": {
    "cleverLinks": false,
    "css": [
      "styles/custom.css"
    ],
    "footerText": "Westures: Robust Multitouch Gestures",
    "logo": {
        "width": "0px",
        "height": "0px"
    },
    "monospaceLinks": false,
    "name": "westures-core"
  }
}

