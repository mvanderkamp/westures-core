{
  "name": "westures-core",
  "version": "0.6.0",
  "description": "The core engine of the Westures gesture library.",
  "keywords": [
    "gesture",
    "tap",
    "pinch",
    "pan",
    "rotate",
    "swipe",
    "swivel",
    "track",
    "simultaneous",
    "touch",
    "mouse",
    "pointer",
    "multitouch"
  ],
  "browser": "index.js",
  "browserify": {
    "transform": [[
      "babelify", {
        "presets": [
          [
            "@babel/preset-env",
            {
              "corejs": "3.x",
              "targets": {
                "edge": "40",
                "firefox": "60",
                "chrome": "67",
                "safari": "11.1"
              },
              "useBuiltIns": "usage"
            }
          ]
        ]
      }
    ]]
  },
  "directories": {
    "test": "test"
  },
  "author": "Michael van der Kamp <michael.vanderkamp@usask.ca>",
  "repository": {
    "type": "git",
    "url": "https://github.com/mvanderkamp/westures-core"
  },
  "homepage": "https://mvanderkamp.github.io/westures-core/index.html",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.4.0",
    "@babel/preset-env": "^7.4.2",
    "arkit": "^1.5.0",
    "babelify": "^10.0.0",
    "browserify": "^16.2.3",
    "eslint": "^5.15.3",
    "jest": "^24.5.0",
    "jsdoc": "^3.5.5",
    "tui-jsdoc-template": "^1.2.2"
  },
  "dependencies": {}
}
